<link rel="stylesheet" href="/css/formForInterview.css">
<!-- <script src="../assets/js/script.js"></script> -->
<nav>
    <a href="/employee/dashboard">Home</a>
</nav>
<section  style="padding: 0px;">
    <h1>Allocate Student To An Interview</h1>
    <form action="/student/interview_allocation" method="POST">
        <input id="cName" type="text" name="companyName" placeholder="Enter Company Name" required><br><br>
        <input id="iDate" type="date" name="date" placeholder="Enter Date Of Interview" required><br><br>
        <input id="studentID" type="hidden" name="studentID" value=<%=id %>>
        <button id="button" >Submit</button>
    </form>
    <h2 style="margin-left: 4%">Select from companies</h2>
</section>
<div class="companyList">
    <ul class="student_list_container">
        <% for(company of companies){%> <%- include('interviewListitems'); %> <%}%>
    </ul>
</div>